#include "iostream"
#include "Math.h"
#include "cmath"
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	const int n = 10;
	const int m = 10;
	const int d = 61;
	int mas[n][m];
	int i, j;
	std::cout << "Исходная матрица" << std::endl;
	for (i = 0; i < n; i++)
	{
		for (j = 0; j < m; j++)
		{
			mas[i][j] = rand() % d;
			std::cout << mas[i][j] << ' ';
		}
		std::cout << std::endl;
	}
	cout << "Готовая,отсортированная матрица" << endl;
	float max = 0;
	for (i = 0; i < n; i++)
	{
		for (j = 0; j < m; j++)
		{
			if (mas[i][j] % 5 == 0)
			{
				cout << mas[i][j] << ' ';
				if (mas[i][j] > max)
					max = mas[i][j];
			}	
		}
	}
	cout <<  '\n';
	cout << "Максимальное число: " << max << "\n";
	system("pause");
	return 0;
}
